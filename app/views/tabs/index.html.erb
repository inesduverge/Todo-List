<ul>
  <% @tabs.each do |tab| %>
    <li><%= link_to tab.titulo, tab_path(tab.id) %></li>


    <%= form_for tab do |f2| %>
      <%= hidden_field :tab, :id, value: tab.id %>
      <%= f2.text_field :titulo%>
      <%= f2.submit "Update" %>
    <% end %>

 
  <% end %>
</ul>

<%= form_for @tab do |f| %>
  <%= f.label :titulo %>
  <%= f.text_field :titulo %>
  <%= f.submit "Create tab" %>
<% end %>
  
